{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-await-in-loop": "off",
        "no-plusplus":"off",
        "no-console":"off",
        "indent": [
            "error",
            "tab"
        ],
        "no-tabs":"off",
        "import/no-extraneous-dependencies": [
            "error",
            {
                "devDependencies": false,
                "optionalDependencies": false,
                "peerDependencies": false
            }
        ]
    }
}
